# Example terraform.tfvars file
# Copy this file to terraform.tfvars and fill in your values
# DO NOT commit terraform.tfvars to version control

# AWS Credentials
# These should ideally be provided via environment variables:
# AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY
aws_access_key = "your-access-key"
aws_secret_key = "your-secret-key"

# Email Configuration
email_host_user     = "your-email@example.com"
email_host_password = "your-email-password"

# African Taking API Configuration
african_taking_username = "your-username"
african_taking_api_key  = "your-api-key"

# Django Admin Configuration
django_admin_username = "admin"
django_admin_email    = "admin@example.com"
django_admin_password = "strong-admin-password"

# Django Secret Key
django_secret_key = "your-django-secret-key"

# Database Configuration
db_password = "your-strong-database-password"

# Other Configuration (optional, defaults provided in variables.tf)
# aws_region            = "eu-north-1"
# project_name          = "todo-app"
# key_name              = "todo-app-key"
# ec2_instance_type     = "t3.micro"
# db_name               = "todo_db"
# db_username           = "todo_admin"